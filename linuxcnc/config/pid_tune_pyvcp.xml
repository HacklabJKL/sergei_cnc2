<pyvcp>

<labelframe text="X axis PID">
    <vbox>
        <hbox>
            <label text="P:" />
            <spinbox halpin="Pgain0" min_="0" max_="1000" param_pin="1" resolution="1" />
        </hbox>
        <hbox>
            <label text="I:" />
            <spinbox halpin="Igain0" min_="0" max_="1000" param_pin="1" resolution="1" />
        </hbox>
        <hbox>
            <label text="D:" />
            <spinbox halpin="Dgain0" min_="0" max_="1000" param_pin="1" resolution="0.1" />
        </hbox>
        <hbox>
            <label text="FF1:" />
            <spinbox halpin="FF1gain0" min_="0" max_="5" param_pin="1" resolution="0.001" format="5.3f" />
        </hbox>
        <hbox>
            <label text="FF2:" />
            <spinbox halpin="FF2gain0" min_="0" max_="5" param_pin="1" resolution="0.001" format="5.3f" />
        </hbox>
        <hbox>
            <label text="Ferror:" />
            <number halpin="Ferror_min0" format="+5.3f" />
            <number halpin="Ferror_max0" format="+5.3f" />
            <button halpin="Ferror_reset0" text="Reset" />
        </hbox>
    </vbox>
</labelframe>

